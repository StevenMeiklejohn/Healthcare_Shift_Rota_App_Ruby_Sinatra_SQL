
<div class ="title" id="title">
<h1>Client Index</h1>
</div>


<% for client in @clients %>
<div class="grid" id="col-1-3_1">
<div class="col-1-3">
<p>Client ID:  <a href="/clients/<%=  client.id %>"><%= client.id %></a></p>
